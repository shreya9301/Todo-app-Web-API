#!/home/shreya/todo_app/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'bizerror==0.4.7','console_scripts','bizerror-generator'
__requires__ = 'bizerror==0.4.7'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('bizerror==0.4.7', 'console_scripts', 'bizerror-generator')()
    )
